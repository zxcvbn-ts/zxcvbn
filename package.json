{
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "name": "zxcvbn-ts",
  "version": "4.4.2",
  "description": "realistic password strength estimation",
  "author": "hrueger & MrWook",
  "license": "MIT",
  "main": "lib/main.js",
  "homepage": "https://github.com/zxcvbn-ts/zxcvbn",
  "repository": {
    "type": "git",
    "url": "git@github.com:zxcvbn-ts/zxcvbn.git"
  },
  "files": [],
  "prettier": "./prettier.js",
  "scripts": {
    "build": "lerna exec --parallel -- rollup -c ../../scripts/rollup.config.js",
    "demo": "yarn build && npx copyfiles -f dist/main.browser.js demo && npx move-cli demo/main.browser.js demo/zxcvbn.js && http-server demo",
    "lint": "eslint --ext .js --fix --cache .",
    "test": "jest",
    "release": "standard-version",
    "release:dry": "standard-version --dry-run",
    "changed": "lerna changed",
    "lerna-publish": "yarn build && lerna publish from-git -y",
    "generate:languageData": "yarn start-ts-file ./data-scripts/generate.ts",
    "start-ts-file": "node -r esm -r esbuild-register --unhandled-rejections=strict",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@rollup/plugin-typescript": "6.0.0",
    "@types/jest": "26.0.14",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "axios": "0.20.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.5.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "byline": "5.0.0",
    "esbuild": "0.8.28",
    "esbuild-register": "1.1.1",
    "eslint": "7.11.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "esm": "3.2.25",
    "http-server": "^0.12.3",
    "iconv-lite": "0.6.2",
    "jest": "26.5.2",
    "lerna": "^3.22.1",
    "prettier": "2.1.2",
    "rollup": "2.29.0",
    "rollup-plugin-copy": "3.3.0",
    "rollup-plugin-delete": "2.0.0",
    "sprintf-js": "1.1.2",
    "standard-version": "9.0.0",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7",
    "vuepress": "1.7.1"
  },
  "keywords": [
    "password",
    "passphrase",
    "security",
    "authentication",
    "strength",
    "meter",
    "quality",
    "estimation",
    "pattern",
    "cracking",
    "scoring",
    "entropy",
    "bruteforce"
  ]
}
